{
  "name": "hugostarter",
  "version": "2.0.0",
  "description": "Starter files for a Hugo project",
  "main": "index.js",
  "repository": "https://github.com/theNewDynamic/hugo-starter.git",
  "author": "budparr <budparr@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.js",
    "build-dev": "NODE_ENV=development webpack --config webpack.js --progress --watch",
    "content": "hugo server -F -D --navigateToChanged",
    "deploy": "yarn build && hugo --gc --minify",
    "start": "hugo server --gc -F -D --disableFastRender --navigateToChanged"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.53",
    "@types/jquery": "^3.3.4",
    "autoprefixer": "^9.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0-beta",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.6",
    "glob-all": "^3.1.0",
    "jquery": "^3.3.1",
    "lazysizes": "^4.0.1",
    "mini-css-extract-plugin": "^0.4.2",
    "postcss": "^7.0.2",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^2.1.0",
    "purgecss-webpack-plugin": "^1.3.0",
    "siema": "^1.5.1",
    "tailwindcss": "^0.6.4",
    "tailwindcss-flexbox-order": "^1.0.0",
    "typeface-play": "^0.0.54",
    "webpack": "^4.16.0",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {}
}
